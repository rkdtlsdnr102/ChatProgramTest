<Window x:Class="ChatClient.InsertIdWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatClient"
        mc:Ignorable="d"
        Title="InsertIdWindow" Height="100" Width="150">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="TextBox_UserId" HorizontalAlignment="Center" Margin="0,20,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="110" Height="18"/>
        <Button x:Name="Button_Ok" Content="확인" HorizontalAlignment="Left" Margin="20,50,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.721,-0.409" Height="20" Width="30" Click="Button_Ok_Click"/>
        <Button x:Name="Button_Cancel" Content="취소" HorizontalAlignment="Left" Margin="100,50,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.721,-0.409" Height="20" Width="30" Click="Button_Cancel_Click"/>

    </Grid>
</Window>
